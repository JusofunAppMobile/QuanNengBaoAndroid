<?xml version="1.0" encoding="utf-8"?>
<com.jusfoun.jusfouninquire.ui.widget.SwipeRightMenuLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    xmlns:swipe="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    swipe:content_view="@+id/swipe_content"
    swipe:right_view="@+id/swipe_right">

    <FrameLayout
        android:id="@id/swipe_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <FrameLayout
        android:id="@id/swipe_right"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>

</com.jusfoun.jusfouninquire.ui.widget.SwipeRightMenuLayout>